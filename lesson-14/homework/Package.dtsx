            Encoding="UTF8"><![CDATA[#region Namespaces
using System;
using System.Data;
using System.Data.SqlClient;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.Text;
using System.Net.Mail;
#endregion

namespace ST_de36a83d90644e309cc39f47bf73807c
{
    [Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
    public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
    {
        public void Main()
        {
            // 1. SQL ulanish qatori – server va database nomi
            string connStr = "Data Source=localhost\\SANJARBEK;Initial Catalog=SSIS14;Integrated Security=SSPI;";
            StringBuilder emailBody = new StringBuilder();
            int errorCount = 0;

            // 2. Email matnining boshlanishi
            emailBody.AppendLine("Bugungi yuklash davomida quyidagi mijozlar CustomerLookup jadvalida topilmadi:");
            emailBody.AppendLine("-----------------------------------------");

            try
            {
                using (SqlConnection conn = new SqlConnection(connStr))
                {
                    conn.Open();

                    // 3. Bugungi sana uchun xatoliklar ro‘yxatini olish
                    string query = "SELECT CustomerID, CustomerName FROM ErrorCusstomers WHERE CAST(ErrorDate AS DATE) = CAST(GETDATE() AS DATE)";
                    SqlCommand cmd = new SqlCommand(query, conn);
                    SqlDataReader reader = cmd.ExecuteReader();

                    while (reader.Read())
                    {
                        errorCount++;
                        emailBody.AppendLine($"ID: {reader["CustomerID"]} | Name: {reader["CustomerName"]}");
                    }

                    reader.Close();
                }

                // 4. SSIS variable'larni to‘ldirish
                Dts.Variables["User::EmailBody"].Value = emailBody.ToString();
                Dts.Variables["User::MissingCustomerCount"].Value = errorCount;

                // 5. Agar xatoliklar bo‘lsa, email yuboriladi
                if (errorCount > 0)
                {
                    SendEmail(emailBody.ToString());
                }

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Script Task", ex.Message, "", 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        // ✅ Email yuborish metodi
        private void SendEmail(string body)
        {
            string from = "gsanjarbek1024@gmail.com";
            string password = "uvzpeuqexcnrptpz"; // ❗ Gmail App password — xavfsizlik uchun variable sifatida saqlang
            string to = "sayyoraibrohimova11@gmail.com";
            string subject = "🚨 Missing Customers Report - SSIS";

            MailMessage mail = new MailMessage(from, to, subject, body);
            SmtpClient client = new SmtpClient("smtp.gmail.com", 587);
            client.EnableSsl = true;
            client.Credentials = new System.Net.NetworkCredential(from, password);
            client.Send(mail);
        }

        #region ScriptResults declaration
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion
    }
}]]></ProjectItem>
