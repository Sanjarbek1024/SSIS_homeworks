        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {
            try
            {
                // System variables dan xatolik tafsilotlarini oling
                string sourceName = Dts.Variables["System::SourceName"].Value.ToString();
                string errorDesc = Dts.Variables["System::ErrorDescription"].Value.ToString();
                int errorCode = (int)Dts.Variables["System::ErrorCode"].Value;

                // Email xabarini tuzing
                string subject = "Package Error Notification";
                string body = "Error in task: " + sourceName + "\r\n" +
                              "Error description: " + errorDesc + "\r\n" +
                              "Error code: " + errorCode;

                // SMTP mijozini sozlash (avvaldan yaratilgan SMTP Connection managerga qaramay, kodda qo'lda kiritamiz)
                SmtpClient smtp = new SmtpClient("smtp.gmail.com", 587);
                smtp.EnableSsl = true;
                smtp.Credentials = new NetworkCredential("gsanjarbek1024@gmail.com", "uvzpeuqexcnrptpz");

                // Email jo'natish
                MailMessage mail = new MailMessage();
                mail.From = new MailAddress("gsanjarbek1024@gmail.com");
                mail.To.Add("sayyoraibrohimova11@gmail.com");  // Qabul qiluvchi manzil
                mail.Subject = subject;
                mail.Body = body;

                smtp.Send(mail);

                Dts.TaskResult = (int)ScriptResults.Success;
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Event Handler Script Task", ex.Message, "", 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        enum ScriptResults
        {
            Success = DTSExecResult.Success,
            Failure = DTSExecResult.Failure
        } 
	}
}]]></ProjectItem>
